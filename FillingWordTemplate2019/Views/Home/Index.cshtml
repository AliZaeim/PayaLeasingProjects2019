@model FillingWordTemplate2019.Models.UserData

@using (Html.BeginForm("Generate", "Home", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Name)
        @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.Family)
        @Html.TextBoxFor(m => m.Family, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.Age)
        @Html.TextBoxFor(m => m.Age, new { @class = "form-control" })
    </div>

    <button type="submit" class="btn btn-primary">Generate Document</button>
}